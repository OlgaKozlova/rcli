<% var fields = fields || [] -%>
<% var buttons = buttons || [] -%>
const VIEW_NAME = '<%= t.snakify(featureName)  -%>_';

<% fields.forEach(function(field){ -%>
<% const transformedField = t.snakify(field); -%>
export const SET_<%- transformedField %>_ACTION = `${VIEW_NAME}SET_<%- transformedField -%>`;
<% }); -%>
<% buttons.forEach(function(button){ -%>
<% const transformedButton = t.snakify(button); -%>
export const HANDLE_<%- transformedButton %>_BUTTON_ACTION = `${VIEW_NAME}HANDLE_<%- transformedButton -%>`;
<% }); %>