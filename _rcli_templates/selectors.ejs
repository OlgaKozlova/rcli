<% }); -%>

export const <%- featureName %>Selector = createSelector(
    [
<% fields.forEach(function(field){ -%>
        get<%- t.capitalize(field) %>,
<% });-%>
    ],
    (
<% fields.forEach(function(field){ -%>
        <%- field %>,
<% });-%>
    ) => {
        // place code for view selector here
        return {
            // place other computed properties here
<% fields.forEach(function(field){ -%>
            <%- field %>,
<% });-%>
        };
    },
);
